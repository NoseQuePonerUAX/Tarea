#include <iostream>
#include <string>

int main() {
    std::cout <<"Hello,World" << std::endl;
    std::string nombre;
    int edad;
    std::cout<<"Cual es su nombre de usuario:"<<std::endl;std::cin>>nombre;
    std::cout<<"Cual es su edad"<<std::endl;std::cin>>edad;
    std::cout<<"Su nombre de usuario es:"<<nombre<<std::endl;
    std::cout<<"Su edad es:"<<edad<<std::endl;


    return 0;
}
